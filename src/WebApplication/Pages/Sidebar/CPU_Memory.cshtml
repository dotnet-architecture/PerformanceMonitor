@page
@model WebApplication.Pages.Metrics.CPU_MemoryModel
@{
    ViewData["Title"] = "CPU_Memory";
}

<div class="main">
    <font size="50">CPU and Memory Usage</font>
    <p>Average CPU Usage:</p>
</div>

<div class="body">
    <p>Log of Time and CPU Usage</p>
    <table style="width:100%">
        <tr>
            <th>CPU</th>
            <th>Timestamp</th>
        </tr>
        @if (Model.cpu != null)
        {
            <tr>
                <td>@Model.cpu.usage</td>
                <td>@Model.cpu.timestamp</td>
            </tr>
        }
        else
        {
            <p>Unable to retrieve data</p>

        }
    </table>
</div>
